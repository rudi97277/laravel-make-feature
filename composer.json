{
  "name": "rudi97277/laravel-make-feature",
  "description": "A package to generate feature-related files (controller, service, repository, and model) with ease for Laravel projects.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "php",
    "feature",
    "code pattern"
  ],
  "type": "library",
  "require": {
    "php": "^7.4|^8.0|^8.2",
    "illuminate/console": "^7.0|^8.0|^9.0|^10.0|^11.0",
    "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Rudi97277\\LaravelMakeFeature\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Rudianto Sihombing",
      "email": "rudi97277@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Rudi97277\\LaravelMakeFeature\\LaravelMakeFeatureServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
